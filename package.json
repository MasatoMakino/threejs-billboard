{
  "name": "threejs-billboard",
  "version": "0.6.2",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": "https://MasatoMakino@github.com/MasatoMakino/threejs-billboard.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "three": ">=0.126.0 <1.0.0",
    "pixi.js-legacy": ">=6.0.4 <7.0.0"
  },
  "devDependencies": {
    "@types/offscreencanvas": "^2019.6.4",
    "@types/three": "^0.133.1",
    "date-fns": "^2.25.0",
    "gulptask-demo-page": "MasatoMakino/gulptask-demo-page",
    "gulptask-dev-server": "MasatoMakino/gulptask-dev-server",
    "husky": "^7.0.4",
    "pixi.js-legacy": "^6.2.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "three": "^0.133.1",
    "typedoc": "^0.22.8",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run buildTS & npm run demo",
    "demo": "npx gulptask-demo-page --body '<canvas id=\"webgl-canvas\" width=\"640\" height=\"480\"></canvas>'",
    "watch:demo": "npm run demo -- -W",
    "buildTS": "npx tsc --project tsconfig.json & tsc --project tsconfig.esm.json",
    "watch:tsc": "tsc -W --incremental --project tsconfig.cjs.json & tsc -W --incremental --project tsconfig.esm.json",
    "server": "npx gulptask-dev-server -b ./docs/demo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {}
}
